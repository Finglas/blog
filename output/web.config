<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpRedirect enabled="false" destination="" httpResponseStatus="Permanent" />
        <rewrite>
            <rules>
                <rule name="Series Redirect" patternSyntax="Wildcard" stopProcessing="true">
                    <match url="*/p/series.html" />
                    <action type="Redirect" url="/pages/series.html" />
                </rule>
                <rule name="About Redirect" patternSyntax="Wildcard" stopProcessing="true">
                    <match url="*/p/about-me.html" />
                    <action type="Redirect" url="{R:1}/pages/about.html" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
